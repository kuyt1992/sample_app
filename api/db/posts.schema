create_table "posts", id: :integer, options: "ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci ROW_FORMAT=COMPRESSED", force: :cascade do |t|
  t.string "content"
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
  t.datetime :deleted_at, null: true
  
  t.index ["content"], name: "index_posts_on_content"
end